function onMiss(note)
{
    if (note == null)
        return;

    var me = char('steve');

    FlxTween.cancelTweensOf(me);
    game.modchartTweens['steve oof'] = FlxTween.color(me, 0.8 / game.playbackRate, 0xFFFF0000, 0xFFFFFFFF, {ease: FlxEase.expoOut});

    FlxG.sound.play(Paths.returnModsSound('characters/steve', 'oof'), 0.5);
}